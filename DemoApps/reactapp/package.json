{
  "name": "reactapp",
  "version": "1.0.0",
  "description": "React App",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmckeone/reactapp"
  },
  "main": "src/index.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "stage-1",
      "stage-2",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "vendorize": [
    "babel-polyfill",
    "immutable",
    "react",
    "react-dom",
    "react-bootstrap",
    "react-redux",
    "react-router",
    "react-router-bootstrap",
    "redux",
    "redux-responsive",
    "react-router-redux",
    "redux-thunk",
    "reselect",
    "typed-immutable"
  ],
  "globalize": {
    "Promise": "es6-promise",
    "fetch": "whatwg-fetch"
  },
  "publicize": {
    "bootstrap/dist/css": "css/",
    "bootstrap/dist/fonts": "fonts/"
  },
  "dependencies": {
    "babel-polyfill": "^6.22.0",
    "bootstrap": "^3.3.7",
    "es6-promise": "^4.0.5",
    "hoist-non-react-statics": "^1.2.0",
    "immutable": "^3.8.1",
    "invariant": "^2.2.2",
    "loaders.css": "^0.1.2",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-dropzone": "^3.9.1",
    "react-loaders": "^2.3.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-responsive": "^4.1.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^2.5.4",
    "socket.io-client": "^1.7.2",
    "typed-immutable": ">=0.1.0",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "bundle-loader": "git+https://github.com/jharris4/bundle-loader.git",
    "compression": "^1.6.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "expect": "^1.20.2",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "express-nunjucks": "^2.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.26.0",
    "imports-loader": "^0.7.0",
    "mocha": "^3.2.0",
    "nunjucks": "^3.0.0",
    "raw-loader": "^0.5.1",
    "redux-logger": "^2.7.4",
    "require-error-handler-webpack-plugin": "0.0.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "uglify-loader": "^1.4.0",
    "webpack": "^1.14.0",
    "webpack-bundle-size-analyzer": "^2.5.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "node server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack",
    "analyze": "cross-env NODE_ENV=analyze webpack -p --json | analyze-bundle-size",
    "test": "NODE_ENV=production mocha --compilers js:babel-core/register 'test'"
  },
  "keywords": [],
  "author": "",
  "license": "BSD"
}
